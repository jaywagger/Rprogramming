#R에서 사용하는 주석문
num = 100 #num이 100이다
num
num <- 1000 #num변수에 1000을 저장하는 의미
num
num==1000 #num이 1000과 같은지 비교 ture or false
num==100
num!=1000
num>100
num<100

test <-"myR"
test
test<-num
test
myR<-num
myR


result <- 100 #숫자형
result
result2 <- "test"
result2

#R에서 제공되는 함수를 이용할 수 있으며 
#class함수는 
class(result)
class(result2)
test <- as.character(result)
test
class(test)
